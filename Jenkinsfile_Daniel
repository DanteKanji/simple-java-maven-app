pipeline {

  agent { 
    node { label 'master'} 
  }

  stages{
        stage('STAGE 1 Call it what you want') {
            steps {
                script{
                    os = sh script: 'cat /etc/os-release', returnStdout:true
                    echo os
                }
            }
        }
        stage('STAGE 2 Call if what you want'){
            steps {
                script{
                    dns = sh script: 'cat /etc/resolve-conf', returnStdout:true
                    echo dns
                }
            }
        }
        stage('STAGE 3 Call if what you want'){
            steps {
                script{
                    logs = sh script: 'cat /var/log/syslog ', returnStdout:true
                    echo logs
                }
            }
        }
  }
}